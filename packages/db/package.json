{
  "name": "@sis/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "import": "./dist/schema/index.js"
    },
    "./ids": {
      "types": "./dist/ids.d.ts",
      "import": "./dist/ids.js"
    },
    "./queries": {
      "types": "./dist/queries/index.d.ts",
      "import": "./dist/queries/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "tsx src/seed.ts",
    "db:seed:financial": "tsx src/seed-financial.ts",
    "db:analytics": "tsx src/analytics/cli.ts",
    "studio": "drizzle-kit studio",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@faker-js/faker": "^10.1.0",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.36.4",
    "duckdb": "^1.4.3",
    "postgres": "^3.4.5"
  },
  "devDependencies": {
    "@sis/typescript-config": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.10.1",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.28.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
